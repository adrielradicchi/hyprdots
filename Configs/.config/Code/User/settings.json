{
  "workbench.colorTheme": "Catppuccin Mocha",
  "eslint.validate": ["javascript", "typescript", "typescriptreact"],
  // "editor.formatOnSave": true,
  // "editor.formatOnType": true,
  // "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    // "editor.formatOnSave": true,
    // "editor.formatOnPaste": true,
    "editor.tabSize": 1,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    },
    "editor.maxTokenizationLineLength": 2500
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.tabSize": 1,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    },
    "editor.maxTokenizationLineLength": 2500
  },
  "[yaml]": {
    "editor.tabSize": 1,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
  },
  "[dart]": {
    "editor.tabSize": 4,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.suggest.insertMode": "replace",
    "editor.defaultFormatter": "Dart-Code.dart-code",
    "editor.inlayHints.enabled": "offUnlessPressed"
  },
  "[elixir]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.wordBasedSuggestions": "allDocuments",
    "editor.formatOnType": true,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.trimAutoWhitespace": false,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "editor.defaultFormatter": "JakeBecker.elixir-ls"
  },
  "elixirLS.trace.server": "verbose",
  "elixirLS.envVariables": {
    "MIX_ENV": "dev"
  },
  "elixirLS.fetchDeps": true,
  "elixirLS.mixTarget": "dev",
  "elixir.credo.diffMode.mergeBase": "main",
  "rust-analyzer.completion.privateEditable.enable": true,
  "rust-analyzer.rustfmt.rangeFormatting.enable": true,
  "rust-analyzer.rustfmt.enableRangeFormatting": true,
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
    "editor.formatOnSave": true
  },
  "[ruby]": {
    "editor.tabSize": 2,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.defaultFormatter": "misogi.ruby-rubocop"
  },
  "files.associations": {
    "*.erb": "erb"
  },
  "[erb]": {
    "editor.defaultFormatter": "aliariff.vscode-erb-beautify"
  },
  "ruby.rubocop.executePath": "/home/adrielradicchi/.asdf/shims/",
  "ruby.rubocop.configFilePath": ".rubocop.yml",
  "ruby.rubocop.onSave": true, //run non-lint commands with bundle exec
  "ruby.lint": {
    "rubocop": {
      "useBundler": true // enable rubocop via bundler
    },
    "reek": {
      "useBundler": true // enable reek via bundler
    }
  },
  "solargraph.diagnostics": true,
  "rubyLsp.formatter": "rubocop", // use rubocop for formatting
  "emmet.includeLanguages": {
    "phoenix-heex": "html",
    "html-eex": "html",
    "*.html.erb": "html"
  },
  "[go]": {
    "editor.insertSpaces": true,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    },
    "editor.defaultFormatter": "golang.go"
  },
  "dart.sdkPath": "/home/adrielradicchi/.asdf/installs/dart/2.18.6/dart-sdk/",
  "dart.sdkPaths": [
    "/home/adrielradicchi/.asdf/installs/dart/2.18.6/dart-sdk/",
